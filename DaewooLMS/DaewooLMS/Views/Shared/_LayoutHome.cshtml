        <script>
                document.addEventListener("DOMContentLoaded", function () {
                    const navItems = document.querySelectorAll(".nav-item1");

                    // Function to set active item
                    function setActiveItem(item) {
                        navItems.forEach(nav => nav.classList.remove("active"));
                        item.classList.add("active");
                        localStorage.setItem("activeNavItem", item.id);
                    }

                    // Add click event to each nav item
                    navItems.forEach(item => {
                        item.addEventListener("click", function () {
                            setActiveItem(this);
                        });
                    });

                    // On page load, set the active item from localStorage
                    const activeNavItemId = localStorage.getItem("activeNavItem");
                    if (activeNavItemId) {
                        const activeNavItem = document.getElementById(activeNavItemId);
                        if (activeNavItem) {
                            setActiveItem(activeNavItem);
                        }
                    }
                });
       
      
        </script>

